
// Insertion SORTING.

#include<stdio.h>

int main()
{
  int a[20], i, j, k, x, n,c;
  printf ("enter the elements= ");
  for (i = 0; i < 5; i++)
	{
	  scanf ("%d", &a[i]);
	}
  for (i = 0; i < 5; i++)
	{
	  for (j = i + 1; j < 5; j++)
		{
		  if (a[i] > a[j])
			{
			  c = a[i];
			  a[i] = a[j];
			  a[j] = c;
			}

		}
	}
  for (i = 0; i < 5; i++)
       printf("%d",a[i]);	

	}










