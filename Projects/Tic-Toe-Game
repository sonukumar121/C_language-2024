
#include <stdio.h>

int main()
{
    int a[3][3],i,j,c=0;
    printf("\nInsert elements in 3*3 matrix= ");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
          scanf("%d",&a[i][j]);  
        }
        
        
    }
    
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            
            	
            	//printf("\n            |          |   \n        ----|----------|----\n            |\n            |\n            |\n            |\n        ----|---------|----\n            |\n            |\n            |\n        ");
            	//printf("          |         |");
            	 printf("\t%d",a[i][j]);
            	 
          
          
         
        }
        if(c>=0)
          {
            printf("\n----------|---------|------------"); 
            c=c+1;
          }
          
        printf("\n");
        
    }
            printf("          |         |");
    


	return 0;
}
